{
  "name": "semesterproject2",
  "version": "1.0.4",
  "main": "index.js",
  "scripts": {
    "build": "sass src/styles/main.scss dist/styles/main.css",
    "watch": "concurrently \"sass --watch src/styles/main.scss:dist/styles/main.css\" \"live-server\"",
    "test": "npm run test-unit",
    "test-unit": "jest",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bootstrap": "^5.3.3",
    "concurrently": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "cypress": "^13.14.2",
    "eslint": "^9.10.0",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "live-server": "^1.2.2",
    "prettier": "^3.3.3",
    "sass": "^1.78.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
